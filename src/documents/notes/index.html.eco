---
title: "Notes"
layout: "base"
activeNavItem: "notes"
---
<ul class="notes-index">
    <% for note in @getCollection('html').findAll({pageType:'note'}, [{date:-1}]).toJSON(): %>
        <li class="notes-list-item">
            <a href="<%= note.url %>">
                <%= note.title %>
            </a>
            <em>&mdash; Last edited on <time><%= note.humanDate %></time> &mdash;</em>
        </li>
    <% end %>
</ul>
